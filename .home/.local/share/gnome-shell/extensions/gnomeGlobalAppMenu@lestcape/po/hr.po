#. globalAppMenu@lestcape->settings-schema.json->max-app-name-
#. size->description
msgid "Maximum size of the application name:"
msgstr "Najveća veličina naziva aplikacija:"

#. globalAppMenu@lestcape->settings-schema.json->max-app-name-size->tooltip
msgid "Set the maximum size of the application name."
msgstr "Postavite najveću veličinu naziva aplikacija."

#. globalAppMenu@lestcape->settings-schema.json->max-app-name-size->units
msgid "characters"
msgstr "znakova"

#. globalAppMenu@lestcape->settings-schema.json->text-gradient->tooltip
msgid "Show a linear gradient effect in the application name"
msgstr ""

#. globalAppMenu@lestcape->settings-schema.json->text-gradient->description
msgid "Select if you want to show a linear gradient effect in the application name."
msgstr ""

#. globalAppMenu@lestcape->settings-schema.json->head-submenu->description
msgid "Submenu"
msgstr "Podizbornik"

#. globalAppMenu@lestcape->settings-schema.json->show-item-icon->description
msgid "Show icons in the menu items"
msgstr "Prikaži ikone u stavkama izbornika"

#. globalAppMenu@lestcape->settings-schema.json->show-item-icon->tooltip
msgid "Select if you want to show icons in the menu items."
msgstr "Odaberite ako želite prikazati ikone u stavkama izbornika."

#. globalAppMenu@lestcape->settings-schema.json->align-menu-
#. launcher->description
msgid "Align all menus with his launcher"
msgstr "Poravnaj sve izbornike s njihovim pokretačima"

#. globalAppMenu@lestcape->settings-schema.json->align-menu-launcher->tooltip
msgid "Select if you want to align all menus with his launcher."
msgstr "Odaberite ako želite poravnati sve izbornike s njihovim pokretačima."

#. globalAppMenu@lestcape->settings-schema.json->effect->description
msgid "Effect on opening and closing:"
msgstr "Efekti pri otvaranju i zatvaranju:"

#. globalAppMenu@lestcape->settings-schema.json->effect->tooltip
msgid "Select the effect on the menu opening and closing."
msgstr "Odaberite efekt pri otvaranju i zatvaranju izbornika."

#. configurableMenu@lestcape->settings-schema.json->effect->options
msgid "None"
msgstr "Nijedan"

#. configurableMenu@lestcape->settings-schema.json->effect->options
msgid "Dispel"
msgstr "Rasprši"

#. configurableMenu@lestcape->settings-schema.json->effect->options
msgid "Hide Horizontal"
msgstr "Sakrij vodoravno"

#. configurableMenu@lestcape->settings-schema.json->effect->options
msgid "Hide Vertical"
msgstr "Sakrij okomito"

#. configurableMenu@lestcape->settings-schema.json->effect->options
msgid "Scale"
msgstr "Razvuci"

#. configurableMenu@lestcape->settings-schema.json->effect->options
msgid "Windows"
msgstr "Prozori"

#. globalAppMenu@lestcape->settings-schema.json->effect-time->description
msgid "Duration of effect:"
msgstr "Trajanje efekta:"

#. globalAppMenu@lestcape->settings-schema.json->effect-time->tooltip
msgid "The time on seconds to perform the effect(More time is slower)."
msgstr "Vrijeme trajanja efekta u sekundama (manje vrijeme je sporije)."

#. globalAppMenu@lestcape->settings-schema.json->automatic-active-
#. mainmenu->description
msgid "Automatically activate the main menu"
msgstr "Automatski aktiviraj glavni izbornik"

#. globalAppMenu@lestcape->settings-schema.json->automatic-active-
#. mainmenu->tooltip
msgid "Select if you want to automatically activate the main menu."
msgstr "Odaberite ako želite automatski aktivirati glavni izbornik."

#. globalAppMenu@lestcape->settings-schema.json->activate-on-hover->description
msgid "Open the menu when I move my mouse over it"
msgstr "Otvori izbornik kada se pomakne miš iznad izbornika"

#. globalAppMenu@lestcape->settings-schema.json->activate-on-hover->tooltip
msgid "Enable opening the menu when the mouse enters the applet"
msgstr "Omogućuje otvaranje izbornika kada miš dođe na aplet"

#. globalAppMenu@lestcape->settings-schema.json->desaturate-app-
#. icon->description
msgid "Desaturate the application icon"
msgstr "Crno-bijela ikona aplikacije"

#. globalAppMenu@lestcape->settings-schema.json->desaturate-app-icon->tooltip
msgid "Select if you want to desaturate the application icon"
msgstr "Odaberite ako želite crno-bijelu ikonu aplikacije"

#. globalAppMenu@lestcape->settings-schema.json->open-active-
#. submenu->description
msgid "Automatically open the submenu"
msgstr "Automatski otvori podizbornik"

#. globalAppMenu@lestcape->settings-schema.json->open-active-submenu->tooltip
msgid ""
"Select if you want to automatically open the submenu when the mouse is inside of the "
"launcher."
msgstr ""
"Odaberite ako želite automatski otvoriti podizbornik kada je pokazivač miša unutar "
"pokretača."

#. globalAppMenu@lestcape->settings-schema.json->close-active-
#. submenu->description
msgid "Automatically close the submenu"
msgstr "Automatski zatvori podizbornik"

#. globalAppMenu@lestcape->settings-schema.json->close-active-submenu->tooltip
msgid ""
"Select if you want to automatically close the submenu when the mouse is out of the "
"launcher."
msgstr ""
"Odaberite ako želite automatski zatvoriti podizbornik kada je pokazivač miša izvan "
"pokretača."

#. globalAppMenu@lestcape->settings-schema.json->show-app-name->description
msgid "Show the application name in the panel"
msgstr "Prikaži naziv aplikacije na panelu"

#. globalAppMenu@lestcape->settings-schema.json->show-app-name->tooltip
msgid "Select if you want to show the application name in the panel."
msgstr "Odaberite ako želite prikazati naziv aplikacije na panelu."

#. globalAppMenu@lestcape->settings-schema.json->head-mainmenu->description
msgid "Main menu"
msgstr "Glavni izbornik"

#. globalAppMenu@lestcape->settings-schema.json->display-in-panel->description
